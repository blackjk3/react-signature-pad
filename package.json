{
  "name": "react-signature-canvas",
  "version": "1.0.2",
  "description": "A React wrapper component around signature_pad. Unopinionated and heavily updated fork of react-signature-pad",
  "main": "build/index.js",
  "files": [
    "src/",
    "build/"
  ],
  "scripts": {
    "lint": "standard --parser babel-eslint",
    "lint:fix": "standard --parser babel-eslint --fix",
    "test": "npm run dist && npm pack",
    "start": "webpack-dev-server -d --inline --hot",
    "dist": "webpack -p --config webpack.production.config.js",
    "pub": "npm run dist && npm publish",
    "changelog:latest": "bash scripts/changelog.sh"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "signature",
    "sign",
    "e-sign",
    "e-signature",
    "canvas",
    "trim",
    "whitespace",
    "draw",
    "pad",
    "wrapper",
    "signature-pad",
    "react-signature-pad"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/agilgur5/react-signature-canvas.git"
  },
  "author": "Anton Gilgur",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "changelog-maker": "^2.3.0",
    "css-loader": "^0.24.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.2.7",
    "standard": "^13.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.10.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "0.14 - 16",
    "react-dom": "0.14 - 16"
  },
  "dependencies": {
    "signature_pad": "^2.3.2",
    "trim-canvas": "^0.1.0"
  }
}
